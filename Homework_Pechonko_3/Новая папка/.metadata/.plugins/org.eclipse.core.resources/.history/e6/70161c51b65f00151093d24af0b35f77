
public class Policlinika {

	public static void main(String[] args) {
		int[][] array = {
				{1, 1, 2, 1}, {1, 4, 1, 1}, {1, 1, 6, 1}
	};
	int sum = 0;
	for (int i = 0; i < array.length; i++) {
			for (int j = 0; j < array[0].length; j++) {
				 sum += array[i][j];
			}
	}
	System.out.println(sum);


	}

}
